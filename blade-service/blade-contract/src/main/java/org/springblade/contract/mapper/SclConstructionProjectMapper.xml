        <result column="update_time" property="updateTime"/>
        <result column="create_dept" property="createDept"/>
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="baseColumnList">
        id,
        scl_ordering_party,
        scl_party_address,
        scl_contractor,
        scl_address_of_contractor,
        scl_varieties,
        scl_specifications,
        scl_processing_fee,
        scl_start_time,
        scl_completion_time,
        scl_processing_way,
        scl_serial_number,
        scl_riginal_material,
        scl_name_of_raw_material,
        scl_transport,
        scl_expense_bearing_party,
        scl_expense_bearing_party1,
        scl_risk_of_loss,
        scl_date_of_request,
        scl_name_of_bank,
        scl_account_name,
        scl_bank_number,
        scl_deposit,
        scl_deposits,
        scl_penalties,
        scl_breach_of_contract,
        scl_datatime,
        scl_processing_fee_1,
        scl_breach_of_contract_78,
        scl_breach_of_contract_711,
        scl_breach_of_contract_712,
