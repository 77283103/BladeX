        scl_paid,
        status,
        is_deleted,
        create_user,
        create_time,
        update_user,
        update_time,
        create_dept
    </sql>
    <!-- 分页 -->
    <select id="pageList" resultMap="sclConstructionProjectResultMap">
        SELECT
            <include refid="baseColumnList"></include>
        FROM scl_construction_project
        WHERE is_deleted = 0
    </select>
</mapper>
